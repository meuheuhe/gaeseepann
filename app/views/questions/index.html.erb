<div class = 'container'> 
  <table id="search_records" class="table">
    <thead>
      <tr>
          <th colspan="2"></th>

          <th class="column_email" style="display">
            <a>제목</a>
          </th>
          <th class="column_id" style="display">
            <a>작성자</a>
          </th>


          <th class="column_last_sign_in_at" style="display">
            <a>번호</a>
          </th>

      </tr>
    </thead>
    

    <tbody>
          <!--  회원 -->
                 
  
        <% @qs.each do |q| %>
        
    
 <% if @ad_id == current_user.email%>
        <tr class="row_1">
            <td>
    <a class="red" title="Delete Record" confirm="Are you sure you want to delete this record?" data-confirm="Are you sure you want to delete this record?" data-remote="true"><i class="fa fa-minus-circle"></i></a>
  </td>
  <td>
    <a class="yellow" title="Edit Record" data-remote="true"><i class="fa fa-pencil-square"></i></a>
  </td>

  <td class="column_id" style="display">
    <a id ="myBtn" href ='/questions/content_view/<%= q.id%>' method ='post '><%= q.title %>
    
   </td>
  <td class="column_email" style="display">
    <%= q.email%>
  </td>
  <td class="column_last_sign_in_at" style="display">
    <%= q.id %>

  </td>
  
        </tr>
        <%else if q.email == current_user.email %>
        <tr class="row_1">
            <td>
    <a class="red" title="Delete Record" confirm="Are you sure you want to delete this record?" data-confirm="Are you sure you want to delete this record?" data-remote="true"><i class="fa fa-minus-circle"></i></a>
  </td>
  <td>
    <a class="yellow" title="Edit Record" data-remote="true"><i class="fa fa-pencil-square"></i></a>
  </td>

  <td class="column_id" style="display">
    <a href ='/questions/content_view/<%= q.id%>' method ='post '><%= q.title %>
    
   </td>
  <td class="column_email" style="display">
    <%= q.email%>
  </td>
  <td class="column_last_sign_in_at" style="display">
    <%= q.id %>

  </td>
  
        </tr>
        <%end%>

          <%end%>
          <%end%>
      
        

    </tbody>

  </table>
    <div class= 'text-center'>
    <ul class="pagination">
    <li class="active"><a class="active" href="#">1</a></li>
    <li class="disabled"><a class="disabled" href="#">2</a></li>
    <li class="disabled"><a class="disabled" href="#">3</a></li>
    <li class="disabled"><a class="disabled" href="#">4</a></li>
    <li class="disabled"><a class="disabled" href="#">5</a></li>
    </ul>
     <a class="btn default pull-left"><%= current_user.email %></a>
    <a class='btn btn-success pull-right'  href="/questions/home1">글쓰기</a>
  </div>
  
      
  </div>



</div>